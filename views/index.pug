- var static = true
doctype html
include ./_partials/banner.pug
include ./_mixins/breadcumb.pug
include ./_mixins/callout.pug
include ./_mixins/callout-custom.pug

html(lang='en')
  head
    base(href=base)
    include ./_partials/head.pug
    block canonical
    block styles
  body
    include ./_partials/sidebar.pug
    .wrapper.d-flex.flex-column.min-vh-100.bg-light
      include ./_partials/header.pug
      .body.flex-grow-1.px-3
        .container-lg
          block view
            p #{t("menu.runners")}
      include ./_partials/footer.pug
    include ./_partials/scripts.pug